g <- unique(read_xlsx("C:/Users/HMA03468/Desktop/profitx_files/Output/WORK_CPU_COMP.xlsx"))
g$Week <- gsub("02Jan18","30Dec17", g$Week)
g$Incentive_Spending_Unit <-currency(g$Incentive_Spending_Unit, digit=0L)
g$CPU_LEASE <-currency(g$CPU_LEASE, digit=0L)
g$gdate <- as.Date(g$Week, "%d %b %y")
gr <- unique(g$Hyundai_Subsegment)
g$WeekNum <- strftime(g$gdate,format = "%Y%V")
g2 <-subset(g, (WeekNum >= 201752) & (Model_Year == 2018))# & (Hyundai_Region == 'Eastern'))
mid <-    subset(g2, (Hyundai_Subsegment == 'Midsize Car'))
com <-    subset(g2, Hyundai_Subsegment == 'Compact Car')
comSUV <- subset(g2, Hyundai_Subsegment == 'Compact SUV')
scomSUV <-subset(g2, Hyundai_Subsegment == 'Sub-Compact SUV')
for (i in gr) {
print(i)
j <- subset(g2, (Hyundai_Subsegment ==  i))
s= ggplot(aes(x=WeekNum, y=CPU_LEASE, color = Hyundai_Region),  data=j)+
geom_line(aes(group= Hyundai_Region),size=1)+
geom_text_repel(aes(label=CPU_LEASE),size = 2,  color = 'black', segment.color = 'grey50',
segment.size=0.25)+
facet_grid(. ~Model )+
theme(legend.position = 'bottom',axis.text.x=element_text(size=6, angle=90),
legend.title=element_text(size=8) , legend.text=element_text(size=8))
m='tiff'
ggsave(filename = paste(i,m,sep="."), plot=s, width=10, height = 6)
}
