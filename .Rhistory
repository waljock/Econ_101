sales <- read.csv("SalesR.csv")
myvars <- c('RDR_TYPE',
'DAY_OF_THE_WEEK','SALES_DAY_OF_MONTH', 'IS_SALES_MONTH_END','SUM_of_Sales')
salesnew <- sales[myvars]
thetree <- ctree(
SUM_of_Sales ~ RDR_TYPE+
DAY_OF_THE_WEEK+SALES_DAY_OF_MONTH+ IS_SALES_MONTH_END, data = salesnew)
plot(thetree)
dev.off()
trainSize <- round(nrow(salesnew) * .7)
testSize <- nrow(salesnew) - trainSize
set.seed(123)
training_indices <- sample(seq_len(nrow(salesnew)),
size=trainSize)
trainSet <- salesnew[training_indices,]
testSet <- salesnew[-training_indices, ]
#levels(trainSet$SectionName) <- levels(testSet$SectionName)
model <- ctree(SUM_of_Sales~., data=trainSet)
newPrediction <- predict(model, list(RDR_TYPE = 'RDR' , DAY_OF_THE_WEEK = 'THU',
SALES_DAY_OF_MONTH = 20, IS_SALES_MONTH_END = 0
),
interval="predict", level=.95)
