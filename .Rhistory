library(data.table)
library(formattable)
library(tidyverse)
library(ggthemes)
library(ggrepel)
d <- read.csv('C:/Users/HMA03468/Documents/Econ_101/Econ_101/econ-rates.csv')
d$ddate <- as.Date(d$TIME_PERIOD, format="%Y-%m-%d")
lst <- list("RIFSPBLP_N.D","RIFSPBLP_N.D","RIFSRP_F02_N.D", "RIFSPFF_N.D")
#s <-subset(d,((d$Series == 'RIFSPPNAAD60_N.WF') | d$Series == 'RIFSPBLP_N.B')& (d$OBS_VALUE != '-9999'))
s <-subset(d,(d$OBS_VALUE != '-9999') &  (d$OBS_VALUE != 'NA') &((d$ddate == "2010-01-01")|(d$ddate == "2018-01-01")) &
(d$Series  %in% lst))
ggplot(s,aes(ddate, OBS_VALUE))+geom_line(aes(color=Instru))+
scale_x_date(date_labels = "%Y%m") + geom_label(aes(label=OBS_VALUE)) + theme()
print(unique(s$Series))
#s <-subset(d,((d$Series == 'RIFSPPNAAD60_N.WF') | d$Series == 'RIFSPBLP_N.B')& (d$OBS_VALUE != '-9999'))
